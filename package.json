{
  "name": "mobalinux-repackager",
  "version": "1.0.0",
  "description": "Mobalytics tool for Linux",
  "main": "src/extract.js",
  "repository": "https://github.com/sekwah41/mobalinux",
  "author": "Sekwah",
  "license": "MIT",
  "private": true,
  "scripts": {
    "extract": "node src/extract.js",
    "postextract": "electron-builder install-app-deps",
    "dist": "electron-builder --config electron-builder/config.js",
    "create-patched-src": "rm -rf modified-src && rm -rf original-src && cp -R app/electron-src original-src && cp -R app/electron-src modified-src && cd modified-src && patch -p1 -i ../patch.diff",
    "generate-patches": "diff -uraN original-src modified-src > patch.diff || true",
    "reset-and-apply-patch": "rm -rf app/electron-src && cp -R original-src app/electron-src && yarn apply-patch",
    "apply-patch": "cd app/electron-src && patch -p1 -i ../../patch.diff"
  },
  "dependencies": {
    "asar": "^3.1.0",
    "node-7z": "^3.0.0",
    "progress": "^2.0.3",
    "progress-download": "^1.0.4",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "electron": "^13.5.1",
    "electron-builder": "^22.13.1"
  }
}
